
<html>
	<head>
		<meta charset='utf-8' />
		<title>{{pageTitle}}</title>
	</head>
	<body>
		<header>
			<h1>{{blogTitle}}</h1>
			<nav>
				<ul>
					{{#each navLinks}}
						<li><a href='{{this.url}}'>{{this.label}}</a></li>
					{{/each}}
				</ul>
			</nav>
		</header>
		<main>
			<h2>Welcome to {{blogTitle}}!</h2>
			<p>{{blogDescription}}</p>
			<h3>Recent Posts</h3>
			<ul>
				{{#each recentPosts}}
					<li><a href='{{this.url}}'>{{this.title}}</a></li>
				{{/each}}
			</ul>
		</main>
		<footer>
			<p>&copy; {{currentYear}} {{blogTitle}}</p>
		</footer>
	</body>
</html>